{
  "name": "@barelyreaper/themer",
  "version": "0.1.3",
  "private": false,
  "description": "One line dark mode support for javascript",
  "keywords": [
    "dark theme",
    "dark",
    "mode",
    "vanilla",
    "js",
    "themer"
  ],
  "homepage": "https://github.com/barelyhuman/themer",
  "bugs": "https://github.com/barelyhuman/themer/issues/",
  "license": "MIT",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts --target=node14 --minify",
    "dev": "nr build --watch",
    "fix": "prettier --write .",
    "prepare": "nr build",
    "prepublishOnly": "rm -rf dist; nr build; nr size;",
    "size": "sizesnap",
    "test": "uvu -r esm tests",
    "web:dev": "parcel public/index.html --dist-dir www",
    "web:build": "parcel build public/index.html --dist-dir www"
  },
  "devDependencies": {
    "@barelyhuman/prettier-config": "^0.1.0",
    "@carbon/icons": "^11.0.2",
    "@types/node": "^17.0.21",
    "esm": "^3.2.25",
    "parcel": "^2.4.1",
    "prettier": "^2.5.1",
    "sizesnap": "^0.1.1",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "sizesnap": {
    "files": [
      "dist/*"
    ]
  },
  "dependencies": {
    "@antfu/ni": "^0.14.0"
  }
}
