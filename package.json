{
  "name": "@barelyreaper/themer",
  "version": "0.1.3",
  "private": false,
  "description": "One line dark mode support for javascript",
  "keywords": [
    "dark theme",
    "dark",
    "mode",
    "vanilla",
    "js",
    "themer"
  ],
  "homepage": "https://github.com/barelyhuman/themer",
  "bugs": "https://github.com/barelyhuman/themer/issues/",
  "license": "MIT",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.mjs",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "build": "run-s clean rollup copy clean:json size",
    "dev": "nr build --watch",
    "fix": "prettier --write .",
    "size": "sizesnap",
    "test": "uvu -r tsm tests",
    "release": "./scripts/release",
    "web:dev": "parcel public/index.html --dist-dir www",
    "web:build": "parcel build public/index.html --dist-dir www",
    "copy": "shx cp package.json README.md LICENSE dist",
    "clean:json": "json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;this['sizesnap']=undefined;\""
  },
  "prettier": "@barelyhuman/prettier-config",
  "devDependencies": {
    "@antfu/ni": "^0.14.0",
    "@babel/core": "^7.17.9",
    "@babel/helper-module-imports": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@barelyhuman/prettier-config": "^0.1.0",
    "@carbon/icons": "^11.0.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.21",
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.36",
    "jsdom": "^19.0.0",
    "json": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.4.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "sinon": "^13.0.2",
    "sizesnap": "^0.1.1",
    "tslib": "^2.3.1",
    "tsm": "^2.2.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "sizesnap": {
    "files": [
      "dist/*"
    ]
  }
}
